rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /products/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /catalogs/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    // Default rules for other paths
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
